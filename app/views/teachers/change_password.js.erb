<% if @error %>
  $("#UpdatePasswordError").html("<%= j render 'shared/errors', resource: @teacher %>")
<% else %>
  $("#profile-overview").html("<%= j render 'users/profile_overview'%>");
  setTimeout(function () {
    $("#ChangePasswordForm")[0].classList.remove("was-validated")
    $("#resetChangePasswordForm").click();
  }, 1000);
<% end %>

<% if @notification %>
  pushNotify('<%= @notification[:status] %>', '<%= @notification[:title] %>', '<%= @notification[:message] %>');
<% end %>
